<Window x:Class="AI_Note_Review.ShowNote"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AI_Note_Review"
        mc:Ignorable="d"
        Title="ShowNote" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Bottom" HorizontalAlignment="Center" FontSize="20" Style="{StaticResource LinkButton}">Close</Button>
        <WebBrowser local:WebBrowserHelper.Body="{Binding ReviewHTML}">

        </WebBrowser>

    </DockPanel>
</Window>
