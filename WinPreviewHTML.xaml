<Window x:Class="AI_Note_Review.WinPreviewHTML"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AI_Note_Review"
        mc:Ignorable="d"
        Title="WinPreviewHTML" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button Style="{StaticResource LinkButton}" DockPanel.Dock="Bottom" HorizontalAlignment="Center" Click="Button_Click">Close</Button>
            <WebBrowser x:Name="MyWB"></WebBrowser>

        </DockPanel>
    </Grid>
</Window>
